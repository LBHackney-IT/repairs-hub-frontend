{
  "name": "repairs-hub-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn next dev -p 5001",
    "debug": "NODE_OPTIONS='--inspect' yarn next dev -p 5000",
    "build": "yarn next build",
    "deploy": "sls deploy",
    "prestart": "yarn next build",
    "start": "yarn next start -p 5001",
    "start-server": "yarn next start -p 5001",
    "tests": "yarn test:unit && yarn test:e2e",
    "test:unit": "NODE_ENV=test node --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage",
    "test:unit-windows": "node ./node_modules/jest/bin/jest.js",
    "test:unit-win-upd": "node ./node_modules/jest/bin/jest.js --updateSnapshot",
    "lint": "yarn eslint . && echo 'Lint complete.'",
    "lintfix": "yarn eslint . --fix",
    "test:e2e": "NODE_ENV=test yarn start-test http://localhost:5001 cy:run",
    "e2e:audit": "NODE_ENV=test yarn start-test http://localhost:5001 cy:run",
    "cy:run": "yarn cypress run --browser chrome --headless",
    "cy:open": "yarn cypress open --browser chrome",
    "clear-eslint-cache": "yarn eslint --cache --cache-location node_modules/.cache/.eslintcache --fix"
  },
  "dependencies": {
    "@reach/dialog": "^0.18.0",
    "@sentry/nextjs": "^9.26.0",
    "@xmldom/xmldom": "^0.8.11",
    "aws-xray-sdk": "^3.10.1",
    "axios": "1.12.2",
    "browser-image-compression": "^2.0.2",
    "classnames": "^2.3.2",
    "cookie": "^0.4.1",
    "date-fns": "^2.16.1",
    "dayjs": "^1.11.7",
    "dotenv": "^8.2.0",
    "easy-soap-request": "5",
    "eslint-config-next": "15",
    "filesize": "^10.1.4",
    "http-status-codes": "^2.1.4",
    "idb": "^7.1.1",
    "jsonwebtoken": "9",
    "lbh-frontend": "^3.5.3",
    "loglevel": "^1.8.0",
    "mockdate": "^3.0.2",
    "next": "15",
    "path": "^0.12.7",
    "qs": "^6.10.1",
    "react": "19",
    "react-dom": "19",
    "react-hook-form": "^6.14.0",
    "react-hook-form-persist": "^3.0.0",
    "react-hotjar": "^5.0.0",
    "react-photo-view": "^1.2.6",
    "react-query": "^3.39.3",
    "retry": "^0.13.1",
    "sass": "^1.29.0",
    "universal-cookie": "^4.0.4",
    "uuid": "^8.3.2",
    "xml2js": "^0.6.2",
    "xpath": "^0.0.32"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-private-methods": "^7.22.5",
    "@faker-js/faker": "^10.1.0",
    "@jest/globals": "^30.2.0",
    "@jest/types": "29",
    "@testing-library/dom": "^8.20.1",
    "@testing-library/jest-dom": "6",
    "@testing-library/react": "16",
    "@testing-library/user-event": "^13.1.9",
    "@types/node": "^22.1.0",
    "@types/qs": "^6.9.15",
    "@types/react": "19",
    "@types/retry": "^0.12.5",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "babel-jest": "29",
    "cypress": "13",
    "cypress-dotenv": "^1.2.2",
    "cypress-dotenv-flow": "^1.2.2",
    "cypress-multi-reporters": "^2.0.4",
    "dotenv-flow": "^3.2.0",
    "eslint": "8",
    "eslint-config-prettier": "^7.0.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-cypress": "^3.4.0",
    "eslint-plugin-jest": "^28.7.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "fishery": "^1.3.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^10.5.3",
    "mocha-junit-reporter": "^2.2.1",
    "node-mocks-http": "^1.10.0",
    "prettier": "^2.2.1",
    "sass": "^1.29.0",
    "start-server-and-test": "2",
    "timezone-mock": "^1.3.6",
    "typescript": "^5.5.4"
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint --fix",
    "**/*.{js,jsx,css,scss,md}": "prettier --write --list-different"
  }
}
