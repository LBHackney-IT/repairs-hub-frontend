{
  "name": "repairs-hub-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5000",
    "debug": "NODE_OPTIONS='--inspect' next dev -p 5000",
    "build": "next build",
    "deploy": "sls deploy",
    "prettier:fix": "npx prettier --write .",
    "prettier:test": "npx prettier -c .",
    "prestart": "next build",
    "start": "next start -p $PORT",
    "tests": "jest && yarn test:e2e",
    "test:unit": "node --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage",
    "lint": "eslint . --max-warnings 0 --ext .js,.jsx && echo 'Lint complete.'",
    "test:e2e": "PORT=5001 start-test 5001 cy:run",
    "e2e:audit": "PORT=5001 start-test 5001 cy:run:audit",
    "e2e:interactive": "PORT=5001 start-test 5001 cy:open",
    "e2e:server": "PORT=5001 start-test 5001",
    "cy:run": "cypress run --browser chrome --headless",
    "cy:run:audit": "cypress run --browser chrome --headless --env runAudit=true",
    "cy:open": "cypress open"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cookie": "^0.4.1",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "easy-soap-request": "^4.1.3",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "lbh-frontend": "^3.5.3",
    "lru-cache": "^6.0.0",
    "mockdate": "^3.0.2",
    "next": "10.2.1",
    "path": "^0.12.7",
    "qs": "^6.10.1",
    "react": ">=16",
    "react-dom": "17.0.1",
    "react-hook-form": "^6.14.0",
    "restana": "^4.8.0",
    "sass": "^1.29.0",
    "serve-static": "^1.14.1",
    "serverless-http": "^2.6.0",
    "universal-cookie": "^4.0.4",
    "uuid": "^8.3.2",
    "xmldom": "^0.6.0",
    "xpath": "^0.0.32"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.31.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^13.1.9",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cypress": "^7.5.0",
    "cypress-audit": "^0.2.3",
    "cypress-dotenv": "^1.2.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "fishery": "^1.3.1",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "jest-clean-console-reporter": "^0.3.0",
    "lint-staged": "^10.5.3",
    "node-mocks-http": "^1.10.0",
    "prettier": "^2.2.1",
    "sass": "^1.29.0",
    "start-server-and-test": "^1.11.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": "",
    "**/*.{js,jsx,css,scss,md}": "prettier --write --list-different"
  }
}
